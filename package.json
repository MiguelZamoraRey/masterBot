{
  "name": "master-bot",
  "version": "1.0.0",
  "description": "masterBot is a discord bot created to be able to play role-playing games with a master that uses the AI to respond to you organically.",
  "keywords": [
    "discord",
    "bot",
    "template",
    "TypeScript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiguelZamoraRey/masterBot/tree/master"
  },
  "bugs": {
    "url": "https://github.com/MiguelZamoraRey/masterBot/issues"
  },
  "homepage": "https://github.com/MiguelZamoraRey/masterBot#readme",
  "author": "Miguel Zamora Rey <miguelzamorarey@gmail.com>",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src --extensions \".ts,.js\" --out-dir dist --copy-files",
    "start": "node dist/index.js",
    "dev": "tsnd -r tsconfig-paths/register --respawn --ignore-watch node_modules src/index.ts"
  },
  "dependencies": {
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
